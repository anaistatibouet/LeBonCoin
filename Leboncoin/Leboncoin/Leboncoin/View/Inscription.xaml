<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Leboncoin.View.Inscription"
             Title="Inscription">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Nom" Text="{Binding Nom}"/>
            <Entry Placeholder="Prenom" Text="{Binding Prenom}"/>
            <Entry Placeholder="Login" Text="{Binding Login}"/>
            <Entry Placeholder="Mot de passe" Keyboard="Numeric" Text="{Binding MotDePasse}"/>
            <Button Text="Inscription" Command="{Binding Inscription}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>